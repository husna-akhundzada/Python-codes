        f1.pack(side=LEFT)
        f1.place(x=60,y=150)
    


    btn3=Button(page2,text="upload photo", width=25, height=3, bd=3 , command=UploadAction, bg="gray", fg="black")
    btn3.place(x=1100,y=450)
    

    
