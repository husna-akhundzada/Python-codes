        def ex():
            self.lab195 = ent108.get()
            self.lab82 = ent74.get()
            self.lab83 = ent75.get()
            self.lab84 = ent76.get()
            self.lab85 = ent77.get()
            self.lab196 = ent109.get()
            self.lab197 = ent110.get()
            self.lab198 = ent111.get()
            self.lab86 = ent78.get()
            self.lab87 = ent79.get()
            self.lab89 = ent80.get()
            self.lab90 = ent81.get()
            self.lab199 = ent112.get()
            self.lab200 = ent113.get()

            sql = "INSERT INTO examination VALUES(%s, %s, %s, %s, %s, %s, %s, %s, %s, %s, %s, %s, %s, %s)"
            mycursor.execute(sql, (
                lab195, lab82, lab83, lab84, lab85, lab196, lab197, lab198, lab86, lab87, lab89, lab90, lab199, lab200))
            conn.commit()
