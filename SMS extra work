        lab103 = Label(f4, text="driver name", fg="black", bg="gray", compound=LEFT)
        lab103.place(x=50, y=250)
        ent92 = Entry(f4, textvar=var92)
        ent92.place(x=190, y=250)

        lab104 = Label(f4, text="driver phone", fg="black", bg="gray", compound=LEFT)
        lab104.place(x=50, y=300)
        ent93 = Entry(f4, textvar=var93)
        ent93.place(x=190, y=300)

        lab105 = Label(f4, text="location", fg="black", bg="gray", compound=LEFT)
        lab105.place(x=50, y=350)
        ent94 = Entry(f4, textvar=var94)
        ent94.place(x=190, y=350)

        lab106 = Label(f4, text="session", fg="black", bg="gray", compound=LEFT)
        lab106.place(x=50, y=400)
        ent95 = Entry(f4, textvar=var95)
        ent95.place(x=190, y=400)

        btn10 = Button(f4, text="SAVE", fg="white", bg="black", bd=1, width=15, height=2)
        btn10.place(x=270, y=440)

    button9 = Button(f1, text="school leaving certificate", width=20, height=1, fg="black", bg="#98c4ed", bd=3,
                     command=btn_9)
    button9.place(x=40, y=410)

    # ====================CERTIFICATE RECORDS=====================================================
    @staticmethod
    def btn_20():
        page19 = Tk()
        page19.geometry("1350x750")
        page19.title("CERTIFICATE RECORDS FORM")
        page19.config(background="#98c4ed")

        labelfont = ('arial', 50, 'bold')
        widget = Label(page19, text="CERTIFICATE RECORDS")
        widget.config(bg="gray", fg="black")
        widget.config(font=labelfont)
        widget.config(height=1, width=70)
        widget.pack(expand=NO, fill=NONE)

    button20 = Button(root, text="certificate records", width=20, height=5, bd=5, fg="black", bg="#98c4ed",
                      command=btn_20)
    button20.place(x=980, y=250)


