# =====================================TRANSPORTATION============================================

class transportation:
    def btn_1(self):
        page1 = Tk()
        page1.geometry("1350x750")
        page1.title("STUDENT/TEACHER TRANSPORTATION FORM")
        page1.config(background="#98c4ed")
        labelfont = ('arial', 50, 'bold')
        widget = Label(self.page1, text="TRANSPORTATION")
        widget.config(bg="gray", fg="black")
        widget.config(font=labelfont)
        widget.config(height=1, width=70)
        widget.pack(expand=NO, fill=NONE)
        f3 = Frame(self.page1, width=400, height=500, bg="gray", relief=SUNKEN, bd=3)
        f3.place(x=200, y=150)

        f4 = Frame(self.page1, width=400, height=500, bg="gray", relief=SUNKEN, bd=3)
        f4.place(x=800, y=150)

        lab1 = Label(f3, text="vehicle id", fg="black", bg="gray", compound=LEFT)
        lab1.place(x=50, y=100)
        ent1 = Entry(f3, textvar=var1)
        ent1.place(x=190, y=100)

        lab2 = Label(f3, text="student id", fg="black", bg="gray", compound=LEFT)
        lab2.place(x=50, y=150)
        ent2 = Entry(f3, textvar=var2)
        ent2.place(x=190, y=150)

        lab3 = Label(f3, text="student name", fg="black", bg="gray", compound=LEFT)
        lab3.place(x=50, y=200)
        ent3 = Entry(f3, textvar=var3)
        ent3.place(x=190, y=200)

        lab4 = Label(f3, text="driver name", fg="black", bg="gray", compound=LEFT)
        lab4.place(x=50, y=250)
        ent4 = Entry(f3, textvar=var4)
        ent4.place(x=190, y=250)

        lab5 = Label(f3, text="driver contact", fg="black", bg="gray", compound=LEFT)
        lab5.place(x=50, y=300)
        ent5 = Entry(f3, textvar=var5)
        ent5.place(x=190, y=300)

        lab6 = Label(f3, text="location", fg="black", bg="gray", compound=LEFT)
        lab6.place(x=50, y=350)
        ent6 = Entry(f3, textvar=var6)
        ent6.place(x=190, y=350)

        lab7 = Label(f3, text="session", fg="black", bg="gray", compound=LEFT)
        lab7.place(x=50, y=400)
        ent7 = Entry(f3, textvar=var7)
        ent7.place(x=190, y=400)

        def stdtrans():
            self.lab1 = ent1.get()
            self.lab2 = ent2.get()
            self.lab3 = ent3.get()
            self.lab4 = ent4.get()
            self.lab5 = ent5.get()
            self.lab6 = ent6.get()
            self.lab7 = ent7.get()

            sql = "INSERT INTO student_transportation VALUES(%s, %s, %s, %s, %s, %s, %s)"
            mycursor.execute(sql, (self.lab1, self.lab2, self.lab3, self.lab4, self.lab5, self.lab6, self.lab7))
            conn.commit()
            conn.close()

        btn1 = Button(f3, text="SAVE", fg="white", bg="black", bd=1, width=15, height=2, command=stdtrans)
        btn1.place(x=270, y=440)

        lab8 = Label(f3, text="STUDENT", bg="black", fg="white", font=("arial", 20, "bold"), width=20, height=1)
        lab8.place(x=20, y=20)

        lab9 = Label(f4, text="TEACHER", bg="black", fg="white", font=("arial", 20, "bold"), width=20, height=1)
        lab9.place(x=20, y=20)

        lab10 = Label(f4, text="vehicle id", fg="black", bg="gray", compound=LEFT)
        lab10.place(x=50, y=100)
        ent8 = Entry(f4, textvar=var8)
        ent8.place(x=190, y=100)

        lab11 = Label(f4, text="teacher id", fg="black", bg="gray", compound=LEFT)
        lab11.place(x=50, y=150)
        ent9 = Entry(f4, textvar=var9)
        ent9.place(x=190, y=150)

        lab13 = Label(f4, text="teacher name", fg="black", bg="gray", compound=LEFT)
        lab13.place(x=50, y=200)
        ent10 = Entry(f4, textvar=var10)
        ent10.place(x=190, y=200)

        lab14 = Label(f4, text="driver name", fg="black", bg="gray", compound=LEFT)
        lab14.place(x=50, y=250)
        ent11 = Entry(f4, textvar=var11)
        ent11.place(x=190, y=250)

        lab15 = Label(f4, text="driver contact", fg="black", bg="gray", compound=LEFT)
        lab15.place(x=50, y=300)
        ent12 = Entry(f4, textvar=var12)
        ent12.place(x=190, y=300)

        lab16 = Label(f4, text="location", fg="black", bg="gray", compound=LEFT)
        lab16.place(x=50, y=350)
        ent13 = Entry(f4, textvar=var13)
        ent13.place(x=190, y=350)

        lab17 = Label(f4, text="session", fg="black", bg="gray", compound=LEFT)
        lab17.place(x=50, y=400)
        ent14 = Entry(f4, textvar=var14)
        ent14.place(x=190, y=400)

        def tchtrans():
            self.lab10 = ent8.get()
            self.lab11 = ent9.get()
            self.lab13 = ent10.get()
            self.lab14 = ent11.get()
            self.lab15 = ent12.get()
            self.lab16 = ent13.get()
            self.lab17 = ent14.get()

            sql = "INSERT INTO teacher_transportation VALUES(%s, %s, %s, %s, %s, %s, %s)"
            mycursor.execute(sql, (self.lab10, self.lab11, self.lab13, self.lab14, self.lab15,
                                   self.lab16, self.lab17))
            conn.commit()
            conn.close()

        btn2 = Button(f4, text="SAVE", fg="white", bg="black", bd=1, width=15, height=2, command=tchtrans)
        btn2.place(x=270, y=440)

    button1 = Button(f1, text="transportation", width=20, height=0, fg="black", bg="#98c4ed", bd=3, command=btn_1)
    button1.place(x=40, y=90)


