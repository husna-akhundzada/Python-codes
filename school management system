        btn52=Button(f4, text="DELETE", fg="white", bg="black", height=2, width=20)
        btn52.place(x=700,y=260)
        btn53=Button(f4, text="CANCEL", fg="white", bg="black", height=2, width=20)
        btn53.place(x=700,y=330)


    f3=Frame(page15, width=900, height=60, bg="gray", relief=SUNKEN , bd=3)
    f3.place(x=230,y=100)
    
    btn50=Button(f3, text="SEARCH", fg="white", bg="black", height=2, width=20, command=save_salary)
    btn50.place(x=600,y=3)
    lab10=Label(f3, text="TEACHER ID :", fg="black", bg="gray")
    lab10.place(x=70,y=10)
    ent10=Entry(f3, textvar=var101)
    ent10.place(x=190,y=10)
    
button15=Button(root, text="salary records", width=20, height=5, bd=5, fg="black", bg="#98c4ed",  command=btn_24)
button15.place(x=810,y=400)

#==============================================================EXAM RECORDS================================================================================

def btn_25():
    page15=Tk()
    page15.geometry("1350x750")
    page15.title("EXAM RECORDS FORM")
    page15.config(background="#98c4ed")

    labelfont=('arial', 50, 'bold')
    widget=Label(page15, text="EXAM RECORDS")
    widget.config(bg="gray", fg="black")
    widget.config(font=labelfont)
    widget.config(height=1, width=70)
    widget.pack(expand=NO , fill=NONE)
    
button15=Button(root, text="exam records", width=20, height=5, bd=5, fg="black", bg="#98c4ed",  command=btn_25)
button15.place(x=980,y=400)

