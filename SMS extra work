        # =====================STUDENT REGISTRATION SAVE AND DELETE BUTTON=====================================
        def stdregsave():
            self.mycursor = conn.cursor()
            self.lab18 = ent15.get()
            self.lab19 = ent16.get()
            self.lab20 = ent17.get()
            self.lab21 = ent18.get()
            self.lab22 = ent19.get()
            self.lab23 = ent20.get()
            self.lab24 = ent21.get()
            self.lab25 = ent22.get()
            self.lab26 = ent23.get()
            self.lab28 = ent24.get()
            self.lab29 = ent25.get()
            self.lab30 = ent26.get()
            self.lab31 = ent27.get()
            self.lab32 = ent28.get()
            self.lab33 = ent29.get()
            self.lab34 = ent30.get()
            self.lab35 = ent31.get()
            self.lab36 = ent36.get(0, 0)
            self.lab27 = comboExample.get()
            sql = "INSERT INTO student_registration VALUES(%s, %s, %s, %s, %s, %s, %s, " \
                  "%s, %s, %s, %s, %s, %s, %s, %s, %s, %s, %s, %s)"
            mycursor.execute(sql, (
                self.lab18, self.lab19, self.lab20, self.lab21, self.lab22, self.lab23, self.lab24,
                self.lab25, self.lab26, self.lab28, self.lab29, self.lab30, self.lab31, self.lab32,
                self.lab33, self.lab34, self.lab35, self.lab36, self.lab27))
            conn.commit()
            # btn_16()
            conn.close()

        btn3 = Button(page2, text="save", width=25, height=3, bd=5, fg="black", bg="gray", command=stdregsave)
        btn3.place(x=1100, y=520)

        # ==========================STUDENT REGISTRATION UPLOAD BUTTON===============================================
        def UploadAction():
            filename = filedialog.askopenfilename()
            import Image
            img = PhotoImage(filename)
            self.lab27=Label(page2, text="selected photo", image=img)
            self.lab27.place(x=80, y=200)
            self.f1=Frame(self.page2, width=200, height=170, bg="black", relief=SUNKEN)
            self.f1.pack(side=LEFT)
            self.f1.place(x=60, y=150)

        btn4 = Button(self.page2, text="upload photo", width=25, height=3, bd=3, command=UploadAction, bg="gray",
                      fg="black")
        btn4.place(x=1100, y=450)

        lab37 = Label(self.page2,
                      text="-----------------------------------permanenet address-----------------------------------",
                      fg="black", bg="#98c4ed")
        lab37.place(x=550, y=135)

        lab38 = Label(self.page2,
                      text="-----------------------------------current address--------------------------------------",
                      fg="black", bg="#98c4ed")
        lab38.place(x=550, y=335)

    button2 = Button(f1, text="student registration", width=20, fg="black", bg="#98c4ed", height=1, bd=3, command=btn_2)
    button2.place(x=40, y=130)

