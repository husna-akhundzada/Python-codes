    # ===================================STUDENT RECORDS===============================================
    def btn_16(self):
        page15 = Tk()
        page15.geometry("1350x750")
        page15.title("STUDENT RECORD FORM")
        page15.config(background="#98c4ed")
        labelfont = ("arial", 50, "bold")
        widget = Label(self.page15, text="STUDENT RECORDS")
        widget.config(bg="gray", fg="black")
        widget.config(font=labelfont)
        widget.config(height=1, width=70)
        widget.pack(expand=NO, fill=NONE)

        student_btn = Frame(self.page15, width=1365, height=120, relief=SUNKEN, bd=3)
        student_btn.place(x=0, y=84)
        s_record = Label(student_btn, text="Search By :")
        s_record.place(x=30, y=30)

        combo_search = ttk.Combobox(student_btn, textvariable=search_by, width=10)
        combo_search["values"] = ("student_Id", "Name")
        combo_search.place(x=95, y=30)

        search_ent = Entry(student_btn, textvariable=search_txt, relief=GROOVE, width=30, bd=2)
        search_ent.place(x=280, y=30)

        # sr_search=Button(student_btn, text="Search", width=15, height=2, bg="black", fg="white",
        # bd=2, command=self.student_records)
        # sr_search.place(x=550,y=20)

        student_frame = Frame(self.page15, relief=RIDGE, bd=3)
        student_frame.place(x=0, y=205, width=1366, height=537)

        scroll_x = Scrollbar(student_frame, orient=HORIZONTAL)
        scroll_y = Scrollbar(student_frame, orient=VERTICAL)
        student_table = ttk.Treeview(student_frame, columns=(
            "student_id", "student_name", "student_fathername", "s_grandf_name", "s_dob", "s_tazkiraNO", "s_nation",
            "s_father_occupation", "s_currentclass", "s_c_province", "s_c_district",
            "s_c_street", "s_c_city", "s_p_province", "s_p_street", "s_p_city", "s_p_district", "s_overview",
            "s_session"), xscrollcommand=scroll_x.set, yscrollcommand=scroll_y.set)

        scroll_x.pack(side=BOTTOM, fill=X)
        scroll_y.pack(side=RIGHT, fill=Y)
        scroll_x.config(command=student_table.xview)
        scroll_y.config(command=student_table.yview)

        student_table.heading("student_id", text="Student Id")
        student_table.heading("student_name", text="Student Name")
        student_table.heading("student_fathername", text="FatherName")
        student_table.heading("s_grandf_name", text="Grand fatherName")
        student_table.heading("s_dob", text="Date Of Birth")
        student_table.heading("s_tazkiraNO", text="Tazkira NO")
        student_table.heading("s_nation", text="Nation")
        student_table.heading("s_father_occupation", text="Father Occupation")
        student_table.heading("s_currentclass", text="Current Class")
        student_table.heading("s_c_province", text="Current Province")
        student_table.heading("s_c_district", text="Current district")
        student_table.heading("s_c_street", text="Current Street")
        student_table.heading("s_c_city", text="Current City")
        student_table.heading("s_p_province", text="Permanent Province")
        student_table.heading("s_p_street", text="Permanent Street")
        student_table.heading("s_p_city", text="Permanent city")
        student_table.heading("s_p_district", text="Permanent District")
        student_table.heading("s_overview", text="overview")
        student_table.heading("s_session", text="session")
        student_table["show"] = "headings"
        student_table.pack(fill=BOTH, expand=1)

        def student_records():
            self.mycursor = conn.cursor()
            mycursor.execute("SELECT * FROM student_registration WHERE" + str(search_by.get()) + " LIKE %" + str(
                search_txt.get()) + "%")
            rows = mycursor.fetchall()
            if len(rows) != 0:
                student_table.delete(student_table.get_children())
                for row in rows:
                    student_table.insert('', END, values=row)

                conn.commit()
            conn.close()

        sr_show = Button(student_btn, text="Show All", width=15, height=2, bg="black", fg="white", bd=2)
        sr_show.place(x=700, y=20)

        sr_search = Button(student_btn, text="Search", width=15, height=2, bg="black", fg="white", bd=2,
                           command=student_records)
        sr_search.place(x=550, y=20)

    button16 = Button(root, text="student records", width=20, height=5, bd=5, fg="black", bg="#98c4ed", command=btn_16)
    button16.place(x=300, y=250)

