def btn_4():
    page4=Tk()
    page4.geometry("1350x750")
    page4.title("STUDENT ATTANDANCE FORM")
    page4.config(background="#98c4ed")

    labelfont=('arial', 50, 'bold')
    widget=Label(page4, text="STUDENT ATTANDANCE")
    widget.config(bg="gray", fg="black")
    widget.config(font=labelfont)
